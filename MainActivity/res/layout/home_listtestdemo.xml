<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingLeft="10dp"
    android:paddingRight="10dp" >

    <RelativeLayout
        android:id="@+id/qlayout"
        android:layout_width="fill_parent"
        android:layout_height="119dp"
        android:background="@drawable/mainclicklist_pink"
        android:padding="10dp" >

        <RelativeLayout
            android:id="@+id/imageLayout"
            android:layout_width="91dp"
            android:layout_height="91dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="14dp"
            android:layout_marginTop="4dp" >

            <ImageView
                android:id="@+id/questionimageinhome"
                android:layout_width="20dp"
                android:layout_height="90dp"
                android:layout_alignLeft="@+id/imageView1"
                android:layout_alignRight="@+id/imageView1"
                android:layout_alignTop="@+id/imageView1"
                android:layout_centerInParent="true"
                android:contentDescription="@string/app_name"
                android:src="@drawable/hugo" 
                android:scaleType="centerCrop"
                android:focusable="false"
                />

            <ImageView
                android:focusable="false"
                android:id="@+id/imageView1"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_alignParentBottom="true"
                android:layout_centerInParent="true"
                android:background="@drawable/mask_q_list"
                android:contentDescription="@string/app_name" 
                 android:scaleType="centerCrop"
                />
        </RelativeLayout>

        <TextView
            android:focusable="false"
            android:id="@+id/objectid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:textColor="@android:color/black"
            android:textSize="14sp"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/imageLayout"
            android:orientation="horizontal"
            android:paddingRight="5dp" >
        </LinearLayout>

        <FrameLayout
            android:id="@+id/allquesn_dateFrLt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_gravity="right"
            android:layout_marginTop="8dp"
            android:layout_toRightOf="@+id/imageLayout" >

            <RelativeLayout
                android:id="@+id/ques_rl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/heading"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                   android:focusable="false"
                    android:maxLines="1"
                    android:text="1"
                    android:textColor="#545353"
                    android:textSize="14sp" />

                <TextView
                    android:visibility="invisible"
                    android:id="@+id/Questionhome"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/heading"
                   android:focusable="false"
                    android:maxLines="1"
                    android:paddingTop="3dp"
                    android:text="12"
                    android:textColor="#A8A8A8"
                    android:textSize="14sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rltv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_below="@+id/Questionhome"
                android:layout_marginTop="8dp" >

               

                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="wrap_content"
                    android:layout_height="17dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:orientation="horizontal"
                    android:paddingRight="5dp" >

                    

                    

                   
                </LinearLayout>

<Button
    android:id="@+id/likeques"
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:layout_alignParentBottom="true"
    android:layout_toLeftOf="@+id/linearLayout2"
    android:background="@android:color/white"
    android:maxLines="1"
    android:paddingTop="3dp"
    android:text="US$ 36"
    android:textColor="#000000"
    android:textSize="12sp" 
    android:focusable="false"
    />

<Button
    android:id="@+id/prices"
    android:layout_width="30dp"
    android:layout_height="20dp"
    android:layout_alignParentBottom="true"
    android:layout_alignParentLeft="true"
    android:background="@android:color/white"
    android:drawableLeft="@drawable/icon_detail_heart_press"
    android:maxLines="1"
    android:paddingTop="3dp"
    android:text="12"
    android:textColor="#000000"
    android:textSize="12sp"
    android:focusable="false"
     />

<TextView
    android:id="@+id/price"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true"
    android:maxLines="1"
    android:paddingTop="1dp"
    android:text="Disc 25%"
    android:textColor="#545353"
    android:textSize="12sp" 
    android:focusable="false"
    android:visibility="invisible"
    />

            </RelativeLayout>
        </FrameLayout>
    </RelativeLayout>

</RelativeLayout>